{
  "name": "to_emoji",
  "version": "1.0.0",
  "description": "Twitter bot that translates every tweet you send it to emoji",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "deploy": "now --public && now ln"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notwaldorf/to_emoji.git"
  },
  "author": "Monica Dinculescu <noms@google.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notwaldorf/to_emoji/issues"
  },
  "homepage": "https://github.com/notwaldorf/to_emoji#readme",
  "dependencies": {
    "moji-translate": "^1.0.0",
    "twit": "^2.2.5"
  },
  "engines": {
    "node": "6.10.3"
  },
  "now": {
    "alias": "to-emoji",
    "env": {
      "emoji_bot_consumer_key": "@emoji_bot_consumer_key",
      "emoji_bot_consumer_secret": "@emoji_bot_consumer_secret",
      "emoji_bot_access_token": "@emoji_bot_access_token",
      "emoji_bot_access_secret": "@emoji_bot_access_secret"
    }
  }
}
